#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/emanueldelawarski/Documents/Builds/graphql-shop-api/node_modules/.pnpm/@graphql-codegen+cli@5.0.0_graphql@16.8.0_typescript@5.2.2/node_modules/@graphql-codegen/cli/esm/node_modules:/Users/emanueldelawarski/Documents/Builds/graphql-shop-api/node_modules/.pnpm/@graphql-codegen+cli@5.0.0_graphql@16.8.0_typescript@5.2.2/node_modules/@graphql-codegen/cli/node_modules:/Users/emanueldelawarski/Documents/Builds/graphql-shop-api/node_modules/.pnpm/@graphql-codegen+cli@5.0.0_graphql@16.8.0_typescript@5.2.2/node_modules/@graphql-codegen/node_modules:/Users/emanueldelawarski/Documents/Builds/graphql-shop-api/node_modules/.pnpm/@graphql-codegen+cli@5.0.0_graphql@16.8.0_typescript@5.2.2/node_modules:/Users/emanueldelawarski/Documents/Builds/graphql-shop-api/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/emanueldelawarski/Documents/Builds/graphql-shop-api/node_modules/.pnpm/@graphql-codegen+cli@5.0.0_graphql@16.8.0_typescript@5.2.2/node_modules/@graphql-codegen/cli/esm/node_modules:/Users/emanueldelawarski/Documents/Builds/graphql-shop-api/node_modules/.pnpm/@graphql-codegen+cli@5.0.0_graphql@16.8.0_typescript@5.2.2/node_modules/@graphql-codegen/cli/node_modules:/Users/emanueldelawarski/Documents/Builds/graphql-shop-api/node_modules/.pnpm/@graphql-codegen+cli@5.0.0_graphql@16.8.0_typescript@5.2.2/node_modules/@graphql-codegen/node_modules:/Users/emanueldelawarski/Documents/Builds/graphql-shop-api/node_modules/.pnpm/@graphql-codegen+cli@5.0.0_graphql@16.8.0_typescript@5.2.2/node_modules:/Users/emanueldelawarski/Documents/Builds/graphql-shop-api/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../esm/bin.js" "$@"
else
  exec node  "$basedir/../../esm/bin.js" "$@"
fi
