#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/emanueldelawarski/Documents/Builds/graphql-shop-api/node_modules/.pnpm/@ardatan+relay-compiler@12.0.0_graphql@16.8.0/node_modules/@ardatan/relay-compiler/bin/node_modules:/Users/emanueldelawarski/Documents/Builds/graphql-shop-api/node_modules/.pnpm/@ardatan+relay-compiler@12.0.0_graphql@16.8.0/node_modules/@ardatan/relay-compiler/node_modules:/Users/emanueldelawarski/Documents/Builds/graphql-shop-api/node_modules/.pnpm/@ardatan+relay-compiler@12.0.0_graphql@16.8.0/node_modules/@ardatan/node_modules:/Users/emanueldelawarski/Documents/Builds/graphql-shop-api/node_modules/.pnpm/@ardatan+relay-compiler@12.0.0_graphql@16.8.0/node_modules:/Users/emanueldelawarski/Documents/Builds/graphql-shop-api/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/emanueldelawarski/Documents/Builds/graphql-shop-api/node_modules/.pnpm/@ardatan+relay-compiler@12.0.0_graphql@16.8.0/node_modules/@ardatan/relay-compiler/bin/node_modules:/Users/emanueldelawarski/Documents/Builds/graphql-shop-api/node_modules/.pnpm/@ardatan+relay-compiler@12.0.0_graphql@16.8.0/node_modules/@ardatan/relay-compiler/node_modules:/Users/emanueldelawarski/Documents/Builds/graphql-shop-api/node_modules/.pnpm/@ardatan+relay-compiler@12.0.0_graphql@16.8.0/node_modules/@ardatan/node_modules:/Users/emanueldelawarski/Documents/Builds/graphql-shop-api/node_modules/.pnpm/@ardatan+relay-compiler@12.0.0_graphql@16.8.0/node_modules:/Users/emanueldelawarski/Documents/Builds/graphql-shop-api/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@ardatan+relay-compiler@12.0.0_graphql@16.8.0/node_modules/@ardatan/relay-compiler/bin/relay-compiler" "$@"
else
  exec node  "$basedir/../../../../../../@ardatan+relay-compiler@12.0.0_graphql@16.8.0/node_modules/@ardatan/relay-compiler/bin/relay-compiler" "$@"
fi
